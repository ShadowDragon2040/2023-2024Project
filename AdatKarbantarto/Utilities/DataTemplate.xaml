<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:AdatKarbantarto.ViewModel"
                    xmlns:view="clr-namespace:AdatKarbantarto.View">


    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FelhasznalokVM}">
        <view:FelhasznaloView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TermekekVM}">
        <view:TermekView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HozzaszolasokVM}">
        <view:HozzaszolasView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SzamlazasVM}">
        <view:SzamlazasView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ImageUploadVM}">
        <view:KepFeltoltesView/>
    </DataTemplate>

</ResourceDictionary>
